<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.win" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>WIN</name>
    <description>
        Aplicaci√≥n para personas con discapacidad
    </description>
    <author email="" href="">
        MP
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon/w.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-localization-strings" spec="git+https://github.com/kelvinhokk/cordova-plugin-localization-strings.git" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
